<p id="notice"><%= notice %></p>

<h1>Ware Houses</h1>

<table>
  <thead>
    <tr>
      <th>Wh code</th>
      <th>Name</th>
      <th>Pincode</th>
      <th>Max capacity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ware_houses.each do |ware_house| %>
      <tr>
        <td><%= ware_house.wh_code %></td>
        <td><%= ware_house.name %></td>
        <td><%= ware_house.pincode %></td>
        <td><%= ware_house.max_capacity %></td>
        <td><%= link_to 'Show', ware_house %></td>
        <td><%= link_to 'Edit', edit_ware_house_path(ware_house) %></td>
        <td><%= link_to 'Destroy', ware_house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ware House', new_ware_house_path %>
